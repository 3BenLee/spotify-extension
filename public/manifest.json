{
  "name": "Spotify Player",
  "version": "1.1",
  "description": "Spotify Player Chrome's extension",
  "permissions": ["contextMenus", "tabs", "declarativeContent", "storage", "http://*/*", "https://*/*"],
  "manifest_version": 2,
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/spotify-mini-player-128.png"
    }
  },
  "icons": {
    "128": "images/spotify-mini-player-128.png"
  }
}
