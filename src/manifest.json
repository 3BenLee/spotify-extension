{
  "name": "Mini Spotity Player",
  "version": "1.0",
  "description": "Spotify player extension",
  "permissions": [
    "tabs",
    "\u003Call_urls\u003E",
    "activeTab",
    "storage",
    "cookies"
  ],
  "manifest_version": 2,
  "options_page": "options.html",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
}
